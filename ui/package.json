  "scripts": {
    "build": "webpack --mode development",
    "build:production": "webpack --mode production",
    "test": "npm run lint && npm run format:verify && npm run unit-tests && npm run e2e-tests:prepare && npm run e2e-tests",
    "lint": "npm run lint:ts && npm run lint:html && npm run lint:style",
    "lint:ts": "tslint --project tsconfig.json '{src,test,test-e2e}/**/*.ts'",
    "lint:html": "htmlhint -f compact src/**/*.{html,ejs}",
    "lint:style": "stylelint src/**/*.scss",
    "format": "prettier --write '{src,test,test-e2e}/**/*.{ts,css}'",
    "format:verify": "prettier --list-different '{src,test,test-e2e}/**/*.{ts,css}'",
    "unit-tests": "karma start",
    "unit-tests:watch": "karma start --nosinglerun",
    "e2e-tests:prepare": "webdriver-manager update --standalone false",
    "e2e-tests": "npm run e2e-tests:chrome && npm run e2e-tests:firefox",
    "e2e-tests:chrome": "node protractor.start",
    "e2e-tests:firefox": "node protractor.start --browser=firefox",
    "serve": "node server",
    "watch": "webpack-dev-server --mode development --hot --inline"
  }
