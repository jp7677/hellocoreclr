    "build": "webpack --mode development",
    "build:production": "webpack --mode production",
    "test": "npm run lint && npm run unit-tests && npm run e2e-tests",
    "lint": "npm run lint:ts && npm run lint:js && npm run lint:html && npm run lint:style",
    "lint:ts": "eslint \"**/*.ts\"",
    "lint:js": "eslint \"**/*.js\"",
    "lint:html": "htmlhint --config .htmlhintrc.json -f compact src/**/*.{html,ejs}",
