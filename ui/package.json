  "scripts": {
    "build": "webpack --mode development",
    "build:production": "webpack --mode production",
    "test": "npm run lint && npm run unit-tests && npm run e2e-tests",
    "lint": "npm run lint:ts && npm run lint:js && npm run lint:html && npm run lint:style",
    "lint:ts": "eslint \"**/*.ts\"",
    "lint:js": "eslint \"**/*.js\"",
    "lint:html": "htmlhint --config .htmlhintrc.json -f compact src/**/*.{html,ejs}",
    "lint:style": "stylelint src/**/*.scss",
    "format": "prettier --write \"{src,test,test-e2e}/**/*.{ts,css,html}\"",
    "unit-tests": "jest",
    "unit-tests:watch": "jest --watchAll --coverage=false",
    "e2e-tests": "node cypress.start --config video=false",
    "e2e-tests:chrome": "node cypress.start --browser chrome --headless --config video=false",
    "e2e-tests:firefox": "node cypress.start --browser firefox --headless --config video=false",
    "serve": "ws --port 3000 --directory wwwroot --spa index.html --compress --rewrite '/api/(.*) -> http://localhost:5000/api/$1' --log.format dev",
